{
  "ebade Project": {
    "prefix": "ebade-project",
    "body": [
      "# ebade Project Configuration",
      "# Generated by ebade",
      "",
      "project:",
      "  name: ${1:my-app}",
      "  type: ${2|saas-dashboard,e-commerce,landing|}",
      "  version: \"1.0.0\"",
      "",
      "design:",
      "  style: minimal-modern",
      "  colors:",
      "    primary: \"${3:#6366f1}\"",
      "    secondary: \"#f59e0b\"",
      "  font: Inter",
      "  borderRadius: md",
      "",
      "pages:",
      "  - path: /",
      "    intent: ${4:landing-page}",
      "    components:",
      "      - hero-section",
      "",
      "api: []",
      "data: []"
    ],
    "description": "Create a new ebade project configuration"
  },
  "ebade Page": {
    "prefix": "ebade-page",
    "body": [
      "- path: ${1:/page-path}",
      "  intent: ${2:page-intent}",
      "  auth: ${3|public,required|}",
      "  components:",
      "    - ${4:component-name}"
    ],
    "description": "Add a new page definition"
  },
  "ebade API Endpoint": {
    "prefix": "ebade-api",
    "body": [
      "- path: ${1:/api/endpoint}",
      "  methods: [${2|GET,POST,PUT,DELETE|}]",
      "  auth: ${3|none,required,admin|}",
      "  rateLimit: ${4:100}/hour"
    ],
    "description": "Add a new API endpoint"
  },
  "ebade Data Model": {
    "prefix": "ebade-data",
    "body": [
      "- name: ${1:ModelName}",
      "  fields:",
      "    - name: id",
      "      type: uuid",
      "      primary: true",
      "    - name: ${2:fieldName}",
      "      type: ${3|string,integer,boolean,timestamp,json|}",
      "    - name: createdAt",
      "      type: timestamp"
    ],
    "description": "Add a new data model"
  },
  "ebade Component": {
    "prefix": "ebade-component",
    "body": ["- ${1:component-name}"],
    "description": "Add a component reference"
  }
}
